<div class="container-fluid"> 
	<div class="row">

   		<div class="col-sm-6" style="background-color:none;">
   		<h3>Current Room Members</h3>
   		<hr style='background-color:#3399FF;border-width:0;color:#3399FF;height:2px;line-height:5;text-align:left;width:99%;'/>

   		<p>
   			<% @members = Member.active_members%>
   <table class="table table-hover">
  <thead>
    <tr>
      
      <th>Bd</th>
      <th>Rank</th>
      <th>Name</th>
      <th>Trade</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.bd %></td>
        <td><%= member.rank %></td>
        <td><%= link_to member.name, member %></td>
        <td><%= member.trade %></td>
        <td><%= member.section %></td>
        
       
      </tr>
    <% end %>

  </tbody>
</table>

   		</p>


 		</div>

    	<div class="col-sm-6" style="background-color:none;">
    	<h3>X-Room Mates</h3>
    	<hr style='background-color:#3399FF;border-width:0;color:#3399FF;height:2px;line-height:5;text-align:left;width:99%;'/>

    	<p>
         
        <% @members = Member.inactive_members%>
          <table class="table table-hover">
  <thead>
    <tr>
      
      <th>Bd</th>
      <th>Rank</th>
      <th>Name</th>
      <th>Trade</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
      
        <td><%= member.bd %></td>
        <td><%= member.rank %></td>
        <td><%= link_to member.name, member %></td>
        <td><%= member.trade %></td>
        <td><%= member.section %></td>
        
      
      </tr>
    <% end %>
  </tbody>
</table>
    	</p>


    </div>
    
  </div>